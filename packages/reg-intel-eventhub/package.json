  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "@types/node": "^24.0.0",
    "typescript": "^5.9.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
