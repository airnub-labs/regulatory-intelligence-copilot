{
  "name": "@reg-copilot/reg-intel-eventhub",
  "version": "0.1.0",
  "description": "Shared event hub infrastructure for distributed SSE pub/sub across services",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./redis": {
      "import": "./dist/redis/index.js",
      "types": "./dist/redis/index.d.ts"
    },
    "./supabase": {
      "import": "./dist/supabase/index.js",
      "types": "./dist/supabase/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
