{
  "name": "@reg-copilot/reg-intel-core",
  "version": "0.4.0",
  "description": "Core orchestration and agents for regulatory intelligence copilot",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^2.2.0",
    "@redactpii/node": "^1.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@reg-copilot/reg-intel-graph": "workspace:*",
    "@reg-copilot/reg-intel-llm": "workspace:*",
    "@reg-copilot/reg-intel-observability": "workspace:*",
    "@reg-copilot/reg-intel-prompts": "workspace:*",
    "eventsource-parser": "^3.0.6",
    "neo4j-driver": "^5.15.0"
  },
  "devDependencies": {
    "@opentelemetry/context-async-hooks": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@types/node": "^24.0.0",
    "typescript": "^5.9.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
