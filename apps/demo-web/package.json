{
  "name": "@reg-copilot/demo-web",
  "version": "0.1.0",
  "description": "Demo web frontend for regulatory intelligence copilot",
  "private": true,
  "scripts": {
    "predev": "pnpm --filter @reg-copilot/reg-intel-core build",
    "dev": "next dev",
    "dev:test": "E2E_TEST_MODE=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",

    "test:e2e": "E2E_TEST_MODE=true playwright test",
    "test:e2e:all": "E2E_TEST_MODE=true playwright test --reporter=list",
    "test:e2e:ui": "E2E_TEST_MODE=true playwright test --ui",
    "test:e2e:headed": "E2E_TEST_MODE=true playwright test --headed",
    "test:e2e:debug": "E2E_TEST_MODE=true playwright test --debug",
    "test:e2e:trace": "E2E_TEST_MODE=true playwright test --trace on",
    "test:e2e:verbose": "E2E_TEST_MODE=true playwright test --reporter=line,html",

    "test:e2e:chromium": "E2E_TEST_MODE=true playwright test --project=chromium",
    "test:e2e:firefox": "E2E_TEST_MODE=true playwright test --project=firefox",
    "test:e2e:webkit": "E2E_TEST_MODE=true playwright test --project=webkit",
    "test:e2e:mobile": "E2E_TEST_MODE=true playwright test --project=mobile-chrome --project=mobile-safari",

    "test:e2e:auth": "E2E_TEST_MODE=true playwright test e2e/01-auth-login.spec.ts",
    "test:e2e:chat": "E2E_TEST_MODE=true playwright test e2e/02-chat-graphrag.spec.ts",
    "test:e2e:branch": "E2E_TEST_MODE=true playwright test e2e/03-conversation-branching.spec.ts",
    "test:e2e:graph": "E2E_TEST_MODE=true playwright test e2e/04-graph-visualization.spec.ts",
    "test:e2e:costs": "E2E_TEST_MODE=true playwright test e2e/05-cost-analytics.spec.ts",
    "test:e2e:team": "E2E_TEST_MODE=true playwright test e2e/06-workspaces-team.spec.ts",
    "test:e2e:flow": "E2E_TEST_MODE=true playwright test e2e/07-end-to-end-flow.spec.ts",
    "test:e2e:tenant": "E2E_TEST_MODE=true playwright test e2e/08-multi-tenant-scenarios.spec.ts",
    "test:e2e:crossjurisdiction": "E2E_TEST_MODE=true playwright test e2e/09-cross-jurisdictional-graphrag.spec.ts",

    "test:e2e:enterprise": "E2E_TEST_MODE=true playwright test --grep '@enterprise'",
    "test:e2e:pro": "E2E_TEST_MODE=true playwright test --grep '@pro'",
    "test:e2e:personal": "E2E_TEST_MODE=true playwright test --grep '@personal'",
    "test:e2e:admin": "E2E_TEST_MODE=true playwright test --grep '@admin'",

    "test:e2e:graphrag": "E2E_TEST_MODE=true playwright test --grep 'GraphRAG'",
    "test:e2e:branching": "E2E_TEST_MODE=true playwright test --grep 'branch|Branch'",
    "test:e2e:tax": "E2E_TEST_MODE=true playwright test --grep 'tax|Tax|VAT|PAYE|PRSI|USC'",
    "test:e2e:crossborder": "E2E_TEST_MODE=true playwright test --grep 'EU|UK|cross-border|treaty'",

    "test:e2e:smoke": "E2E_TEST_MODE=true playwright test --grep '@smoke'",
    "test:e2e:regression": "E2E_TEST_MODE=true playwright test --grep '@regression'",
    "test:e2e:critical": "E2E_TEST_MODE=true playwright test --grep '@critical'",

    "test:e2e:report": "playwright show-report",
    "test:e2e:results": "cat playwright-results/results.json | jq .",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:clean": "rm -rf playwright-results playwright-report test-results",
    "test:e2e:runner": "npx tsx scripts/run-e2e-tests.ts",
    "test:e2e:runner:verbose": "npx tsx scripts/run-e2e-tests.ts --verbose",
    "test:e2e:runner:headed": "npx tsx scripts/run-e2e-tests.ts --headed --verbose",

    "test:all": "pnpm test && pnpm test:e2e"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.104",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^1.30.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.57.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
    "@opentelemetry/instrumentation": "^0.57.0",
    "@opentelemetry/instrumentation-fs": "^0.28.0",
    "@opentelemetry/instrumentation-http": "^0.57.0",
    "@opentelemetry/instrumentation-undici": "^0.19.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-logs": "^0.57.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@reg-copilot/reg-intel-cache": "workspace:*",
    "@reg-copilot/reg-intel-conversations": "workspace:*",
    "@reg-copilot/reg-intel-core": "workspace:*",
    "@reg-copilot/reg-intel-llm": "workspace:*",
    "@reg-copilot/reg-intel-next-adapter": "workspace:*",
    "@reg-copilot/reg-intel-observability": "workspace:*",
    "@reg-copilot/reg-intel-ui": "workspace:*",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@upstash/ratelimit": "^2.0.4",
    "@upstash/redis": "^1.34.3",
    "ai": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "force-graph": "^1.51.0",
    "import-in-the-middle": "2.0.0",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.555.0",
    "next": ">=16.0.7",
    "next-auth": "^4.24.9",
    "pino": "^9.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-force-graph-2d": "^1.29.0",
    "require-in-the-middle": "8.0.1",
    "tailwind-merge": "^3.4.0",
    "thread-stream": "^3.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": ">=16.0.7",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": ">=16.0.7",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
