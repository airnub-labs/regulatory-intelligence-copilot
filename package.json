    "type-check": "pnpm -r type-check"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.9.0"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=24.0.0"
  }
}
